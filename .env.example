# Dumende Frontend Environment Variables
# Copy this file to .env.local for development or .env.production for production

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api
VITE_APP_ENVIRONMENT=development

# Application Configuration
VITE_APP_NAME=Dümende
VITE_APP_VERSION=1.0.0
VITE_APP_TITLE=dümende.com - Your yacht rental solution

# Security Configuration
VITE_API_TIMEOUT=30000
VITE_JWT_STORAGE_KEY=dumende_auth_token
VITE_REFRESH_TOKEN_KEY=dumende_refresh_token

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_SERVICE_WORKER=false

# External Services (if needed)
VITE_GOOGLE_MAPS_API_KEY=
VITE_STRIPE_PUBLISHABLE_KEY=
VITE_SENTRY_DSN=

# Performance Configuration
VITE_CHUNK_SIZE_WARNING_LIMIT=1000
VITE_ENABLE_SOURCE_MAP=false

# Social Media & SEO
VITE_APP_URL=https://dumende.com
VITE_APP_DESCRIPTION=Dümende.com - Professional yacht rental and boat booking platform
VITE_APP_KEYWORDS=yacht rental, boat booking, maritime services, dumende